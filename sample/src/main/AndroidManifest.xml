<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.greenmist.vector.sample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".App"
        >

        <activity android:name=".screens.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".screens.SimpleShapesActivity"/>

        <activity android:name=".screens.TransformsActivity"/>

        <activity android:name=".screens.PathActivity"/>

        <activity android:name=".screens.FillActivity"/>

        <activity android:name=".screens.AdvancedActivity"/>

    </application>

</manifest>